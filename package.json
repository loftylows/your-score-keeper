{
  "name": "yourscorekeeper.com",
  "version": "1.0.0",
  "gitRepository": {
    "type": "github"
  },
  "scripts": {
    "dev": "blitz start",
    "start": "blitz start --production -H 0.0.0.0 -p ${PORT:-8080}",
    "studio": "blitz db studio",
    "build": "NODE_ENV=production blitz db migrate && blitz build",
    "postbuild": "node postBuild.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged && pretty-quick --staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.7",
    "@chakra-ui/icons": "1.0.0-rc.7",
    "@prisma/client": "2.10.0",
    "bad-words": "3.0.3",
    "blitz": "0.23.4",
    "cors": "2.8.5",
    "date-fns": "2.16.1",
    "final-form": "4.20.1",
    "framer-motion": "2.9.4",
    "is-mobile": "2.2.2",
    "mailchecker": "3.3.14",
    "node-fetch": "2.6.1",
    "nprogress": "0.2.0",
    "polished": "3.6.7",
    "querystringify": "2.2.0",
    "react": "0.0.0-experimental-7f28234f8",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "react-error-boundary": "2.3.2",
    "react-final-form": "6.5.1",
    "react-icons": "3.11.0",
    "react-paginate": "6.5.0",
    "react-table": "7.5.2",
    "react-toastify": "6.0.9",
    "secure-password": "4.0.0",
    "sync-request": "6.1.0",
    "uuid": "8.3.0",
    "wrangler": "0.0.2",
    "zod": "1.11.9"
  },
  "devDependencies": {
    "@prisma/cli": "2.8.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/react-hooks": "3.4.2",
    "@types/bad-words": "3.0.0",
    "@types/jest": "26.0.14",
    "@types/querystringify": "2.0.0",
    "@types/react": "16.9.50",
    "@types/secure-password": "3.1.0",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-superjson-next": "0.1.7",
    "eslint": "7.10.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "react-test-renderer": "16.13.1",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3"
  },
  "private": true
}
